from 'https://raw.githubusercontent.com/brundonsmith/bagel/master/lib/ts/core.ts' import { iter, entries, keys, values }

export pure proc assign<B extends {}, A extends B>(a: A, b: B) |> {
    jsGlobal.Object.assign(a, b);
}

export pure func keys<O extends {}>(obj: O): Iterator<string> => keys(obj)

export pure func values<O extends {}>(obj: O): Iterator<unknown> => values(obj)

export pure func entries<O extends {}>(obj: O): Iterator<[string, unknown]> => entries(obj)